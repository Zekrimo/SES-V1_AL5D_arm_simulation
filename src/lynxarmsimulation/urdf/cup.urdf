<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="cup">
    <link name="cup_base_link">
        <visual>
            <geometry>
                <cylinder length="0.05" radius="0.01" />
            </geometry>
            <material name="blue">
                <color rgba="0 0 1 1" /> <!-- Corrected color to blue -->
            </material>
            <origin xyz="0.1 0 0.05" rpy="0 0 0" />
        </visual>
        <collision>
            <geometry>
                <cylinder length="0.05" radius="0.01" />
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" />
            <mass value="0.1" />
            <inertia ixx="0.0001" iyy="0.0001" izz="0.0001" ixy="0" ixz="0" iyz="0"/>
        </inertial>
    </link>
</robot>
